<div class="row">
  <div class="col-md-6">
    <h3><strong>MY DREAM TEAM</strong></h3>
    <br>
    <div class="row">
      {{#each dreamballs}}
        {{#if ownerID}}   
                
          <div class="col-md-2">
            <button class=" change-dev green" 
            dev=1
            data-id="{{id}}" data-newdev=0>
              >>>
            </button>
            <!--
            <button class=" delete-dreamball green" data-id="{{this.id}}">DELETE!</button>
            -->
          </div>
          <div class="col-md-10">
            <p>{{id}}. {{charName}}</p>
          </div>
                      
        {{/if}}
      {{/each}}  
    </div>
  </div>

  

  <div class="col-md-6">
    <h3><strong>DREAMBALL ROSTER</strong></h3>
    <br>
    <div class="row">
      {{#each dreamballs}}
        {{#unless ownerID}} 
          
          <div class="col-md-2">
              <button class=" change-dev green"     
              data-id="{{id}}" data-newdev=1>
                <<<
              </button>
          </div>

          <div class="col-md-10">
              <p>{{id}}. {{charName}}</p>
          </div>
        {{/unless}}
      {{/each}}              
    </div>
  </div>
</div>


<div class="row">
  <form class="create-userform">
    <div class="form-group">
      <label for="user"><h4>User's Name:</h4></label>
      <input type="text" id="user" name="userName">

      <label for="pass"><h4>Password:</h4></label>
      <input type="text" id="pass" name="passWord">
    
      <label for="tname"><h4>Team's name:</h4></label>
      <input type="text" id="tname" name="teamName">
    </div>            

    <div class="col-md-6">
      <br>
      <button class="green" type="Submit">ADD</button>
    </div>  
  </form>
</div>





